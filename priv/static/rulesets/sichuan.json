  "before_turn_change": {
    "actions": [["unset_status", "kan", "first_turn"],
                ["when", [{"name": "match", "opts": [["hand", "calls", "draw"], ["tenpai", "kokushi_tenpai"]]}], [["set_status", "tenpai"]]],
                ["when", [{"name": "not_match", "opts": [["hand", "calls", "draw"], ["tenpai", "kokushi_tenpai"]]}], [["unset_status", "tenpai"]]]]
  },
  "after_turn_change": {
    "actions": [
      ["when", ["no_tiles_remaining"], [["pause", 1000], ["ryuukyoku"]]],
      ["when", [{"name": "not_status_missing", "opts": ["void_manzu", "void_pinzu", "void_souzu"]}, {"name": "not_status", "opts": ["won"]}, "not_no_tiles_remaining"], [["draw"]]],
      ["when", [{"name": "status", "opts": ["won"]}], [["advance_turn"]]]
    ]
  },
  "before_continue": {
    "actions": [["advance_turn"]]
  },
